{
  "name": "corsico",
  "version": "0.0.0",
  "main": "index.js",
  "bin": [
    "server.js"
  ],
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "standard && lodash template='templates/*.html' exports=node --development && node server",

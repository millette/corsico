    "lint": "standard",
    "deps": "standard && dependency-check . && dependency-check . --unused --no-dev --ignore-module lodash"
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "async-lru": "^1.1.1",
    "got": "^8.3.0",
    "koa": "^2.5.0",
    "koa-conditional-get": "^2.0.0",
    "koa-error": "millette/error#original-error",
    "koa-etag": "^3.0.0",
    "koa-logger": "^3.2.0",
    "kompression": "^1.0.0",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "dependency-check": "^3.1.0",
    "lodash-cli": "^4.17.5",
    "standard": "^11.0.1"
  },
  "standard": {
    "ignore": ["lodash.templates.js", "lodash.templates.js.min"]
  }
